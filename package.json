{
	"name": "project",
	"version": "1.0.0",
	"scripts": {
		"db:init": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
		"db:update": "sequelize db:migrate && sequelize db:seed:all",
		"format": "prettier . --write",
		"lint": "eslint **/*.{js,ts}",
