		"start": "node index.js --mode=production"
	},
	"dependencies": {
		"mysql2": "^3.0.0",
		"sequelize": "^6.0.0"
	},
	"devDependencies": {
		"eslint": "^8.0.0",
		"eslint-import-resolver-alias": "^1.0.0",
		"eslint-plugin-import": "^2.0.0",
		"eslint-plugin-spellcheck": "^0.0.20",
		"nodemon": "^3.0.0",
		"prettier": "^3.0.0",
		"sequelize-cli": "^6.0.0"
	}
}
